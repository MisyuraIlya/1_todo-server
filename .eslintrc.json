{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "rules": {
        "semi": ["error", "always"],
        "quotes": [2, "single", { "avoidEscape": true }],
        "indent": [2, 2]
    }
}
